<h1>Memories</h1>
<% @memories.each do |memory| %>
  <div class="memory_content">
    <%= link_to(memory_path(memory.id), {class:"menu tr_size"}) do %>
      <%= memory.date %>
      <%= memory.title %>
    <% end %>
  </div>
<% end %>

<br>

<% if @user != current_user %>
	<%= link_to 'Back', show_other_user_path, class:"menu" %>
<% end %>
